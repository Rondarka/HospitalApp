<Window x:Class="HospitalApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:HospitalApp"
        mc:Ignorable="d"
        Title="Hospital" Height="450" Width="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="45"/>
            <RowDefinition/>
        </Grid.RowDefinitions>
        <Frame HorizontalAlignment="Stretch" Grid.Row="1" VerticalAlignment="Stretch" x:Name="Frame" Navigated="Frame_Navigated" Background="Beige" NavigationUIVisibility="Hidden"/>
        <Button HorizontalAlignment="Left" VerticalAlignment="Top" Width="102" Margin="10,10,0,0" Height="22" Content="Назад" Click="Button_Click"/>
    </Grid>
</Window>
